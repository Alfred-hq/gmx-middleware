    "deploy-local":"graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 gmx-house"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.51.2",
    "@graphprotocol/graph-ts": "^0.31.0"
